{
  "name": "model",
  "version": "1.0.0",
  "description": "model",
  "repository": {
    "type": "git",
    "url": "https://gitlab-dev.r2ai.com.cn/lyz/model.git"
  },
  "author": "Ahn Dohyung",
  "license": "MIT",
  "main": "src/index.tsx",
  "scripts": {
    "develop": "concurrently -r \"npm:type-check:watch\" \"npm:start\"",
    "start": "parcel ./src/index.html",
    "build": "npm run type-check && parcel build ./src/index.html --public-url '/'",
    "prettify": "prettier --write './src/**/*.{ts,tsx}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettify && npm run test"
    }
  },
  "engines": {
    "node": ">= 8.11.0",
    "npm": ">= 6.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "moduleNameMapper": {
      ".+\\.css$": "<rootDir>/src/styleMock.ts"
    },
    "testMatch": [
      "**/__tests__/*.(test|spec).(ts|tsx|js)"
    ]
  },
  "keywords": [
    "Typescript",
    "React",
    "Parcel"
  ],
  "dependencies": {
    "@date-io/core": "^1.3.6",
    "@date-io/date-fns": "^1.3.8",
    "@material-ui/pickers": "^3.1.2",
    "components": "^0.1.0",
    "date-fns": "2.0.0-alpha.27",
    "prop-types": "^15.6.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "src": "^1.1.2"
  },
  "devDependencies": {
    "@material-ui/core": "^4.0.0",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.1.2",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.12",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "concurrently": "^4.1.1",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "jest-dom": "^3.5.0",
    "jest-extended": "^0.11.1",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.2"
  }
}
